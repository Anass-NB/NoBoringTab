{
  "manifest_version": 3,
  "name": "NoBoringTab",
  "version": "1.0",
  "description": "Turn every new tab into motivation.",
  "permissions": ["storage", "tabs", "alarms", "notifications"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
